.wrapper
  .index
    .header
      .header_content
        .header_content-left
          %i.fas.fa-dumbbell.fa-3x
        .header_content-middle
          %h3 title
        .header_content-right
          .new_user
          = link_to '新規登録', new_user_registration_path, method: :get
          .log_in
          = link_to 'ログイン', new_user_session_path, method: :get

    .main#sec
      .main-diary
        %span.my_diary
          = link_to '今日の食事を記録する', new_mypage_path, class:'btn-gradient-3d-orange'
      
      .lists
        -@user.mypage.each do |mypage|
          .card3
            .box3
              = image_tag 'data:image/jpeg;base64,', class: 'image3'
              %div
                %p.title3
                  = link_to mypage.day, mypage_path(mypage),class:'title3'
                %p.content3
                  #{mypage.weight}
      
        
        
        
        
